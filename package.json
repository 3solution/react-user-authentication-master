    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "dotenv": "^2.0.0",
    "enzyme": "^2.2.0",
    "expect": "^1.18.0",
    "file-loader": "^0.8.5",
