    "classnames": "^2.2.5",
    "express-jwt": "^5.0.0",
    "jwt-decode": "^2.1.0",
    "md5": "^2.2.1",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.0-rc.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.0",
    "react-router-bootstrap": "^0.23.1"
  }
